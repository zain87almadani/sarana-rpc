{
    "name": "sarana/rpc",
    "description": "Laravel package to interact with blockchain RPC nodes (e.g. Ethereum, BSC) using Tatum API",
    "type": "library",
    "require": {
        "kornrunner/ethereum-address": "^0.3.0",
        "kornrunner/ethereum-offline-raw-tx": "^0.8.0",
        "web3p/ethereum-util": "^0.1.4"
    },
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Sarana\\Rpc\\": "src/"
        },
        "files": ["src/config/rpcconfig.php"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sarana\\Rpc\\SaranaRpcServiceProvider"
            ]
        }
    }
}
